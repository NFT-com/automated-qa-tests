		// driver.findElement(By.xpath("//button[contains(text(),'Next')]")).click();

	}

	public void StudentStartTest() throws InterruptedException {
		TestR900.click();
		Thread.sleep(2000);
		if (SiteSelection.isDisplayed()) {
			System.out.println("Student Need to Select Site");
			SiteSelection.click();
		} else {
			System.out.println("Student does not need to select site");
		}
		Thread.sleep(2000);
		DirectionNextButton.click();
		Thread.sleep(2000);
		// TimmerExpandButton.click();
		// Thread.sleep(2000);
	}

	public void StartPractice() throws InterruptedException {

		driver.findElement(By.xpath("//button[@class='btn btn-circle btn-lg btn-default pin-button ng-scope']"))
				.click();

		driver.findElement(By.xpath("//span[contains(text(),'to Mariela Sanchez')]")).click();

		NextQnBtn.click();
		Thread.sleep(1500);
		driver.findElement(By.xpath("//button[contains(text(),'Ok')]")).click();
		Thread.sleep(1500);
		BeginTest.click();
	}

