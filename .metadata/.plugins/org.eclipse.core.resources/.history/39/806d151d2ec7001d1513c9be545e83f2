		Save.click();

	}

	public void StartSession() throws InterruptedException {

		Thread.sleep(6000);
		driver.findElement(By.xpath("/descendant::button[@ng-click='startSession(r.TestingSessionId)'][1]"))
				.isEnabled();
		// StartBtn.click();
		JavascriptExecutor js = (JavascriptExecutor) driver;
		js.executeScript("arguments[0].click();", StartBtn);

		JavascriptExecutor jt = (JavascriptExecutor) driver;
		jt.executeScript("arguments[0].click();", StartBtn1);
		// StartBtn1.click();

	}

	public void TakeATest() throws InterruptedException {
		Thread.sleep(2000);
		TakeATest.click();
	}

	public void StationLogin() throws InterruptedException {
		Thread.sleep(2000);
		Agency.sendKeys("TCQARH");
		Thread.sleep(1000);
		User.sendKeys("james");
		Thread.sleep(1000);
		Password.sendKeys("james");
		Thread.sleep(1000);
		StationLogin.click();
	}

	public void StationRegistration() throws InterruptedException {
		// Thread.sleep(2000);
		driver.findElement(By.xpath("//span[contains(text(),'Site One')]")).click();
		driver.findElement(By.xpath("//span[@class='font20 ng-binding ng-scope'][contains(text(),'Site One')]"))
				.click();
		Thread.sleep(2000);
		driver.findElement(By.xpath(
				"//body[@class='ng-scope block-ui block-ui-anim-fade']/div[@id='mainContainer']/div[@class='ng-scope view-container']/div[@class='vertical-center ng-scope']/div[@class='col-xs-12 col-sm-10 col-md-8 col-lg-7 col-xl-5']/div[@class='col-xs-12 col-sm-12 col-md-12 col-lg-12 col-xl-12']/div[@class='col-xs-12 col-sm-12 col-md-12 col-lg-12 col-xl-12']/div[2]/div[1]/div[1]/div[1]/span[1]/i[1]"))
				.click();
		driver.findElement(By.xpath("//span[@class='font20 ng-binding ng-scope'][contains(text(),'TC')]")).click();
		Thread.sleep(2000);
		driver.findElement(By.xpath(
				"//div[contains(@class,'col-xs-12 col-sm-12 col-md-12 col-lg-12 col-xl-12')]//div[3]//div[1]//input[@ng-model='stationName']"))
				.click();
		driver.findElement(By.xpath("//input[@ng-model='stationName']")).sendKeys("Tracker");
		Thread.sleep(2000);
		driver.findElement(By.xpath("//input[@ng-model='registrationStatus']")).isDisplayed();
		WebElement RegisterBtn = driver.findElement(By.xpath("//button[contains(text(),'Register')]"));
		if (RegisterBtn.isEnabled()) {
			Thread.sleep(2000);
			RegisterBtn.click();
			Thread.sleep(2000);
			driver.findElement(By.xpath("//button[@class='swal2-confirm swal2-styled']")).click();
			Thread.sleep(2000);
			driver.findElement(By.xpath("//button[@class='swal2-confirm swal2-styled']")).click();
		} else {
			System.out.println("The Registration Button is not Enabled");
		}

	}

	public void StudentLogin() throws InterruptedException {
		Thread.sleep(5000);
		StudentID.sendKeys("202");
		Thread.sleep(2000);
		StudentEnter.click();
		Thread.sleep(2000);
		StudentConfirmation.click();
