	public void StudentStartTest() throws InterruptedException {
		Listening981L.click();
		Thread.sleep(2000);
		if (SiteSelection.isDisplayed()) {
			System.out.println("Student Need to Select Site");
			SiteSelection.click();
		} else {
			System.out.println("Student does not need to select site");
		}
		
		Thread.sleep(38000);
		
		JavascriptExecutor jv = (JavascriptExecutor) driver;
		jv.executeScript("arguments[0].click();", DirectionNextButton);
		
	}

