	public void StopProctorSession() throws InterruptedException {

		Thread.sleep(1500);
		JavascriptExecutor jv = (JavascriptExecutor) driver;
		jv.executeScript("arguments[0].click();", SignInBtn);

		Thread.sleep(3000);
		AgencySite.clear();
		JavascriptExecutor jf = (JavascriptExecutor) driver;
		jf.executeScript("arguments[0].click();", AgencySite);
		AgencySite.sendKeys("TCQARH");

		User.clear();
		JavascriptExecutor jg = (JavascriptExecutor) driver;
		jg.executeScript("arguments[0].click();", User);
		User.sendKeys("james");

		JavascriptExecutor jh = (JavascriptExecutor) driver;
		jh.executeScript("arguments[0].click();", Password);
		Password.sendKeys("james");

		LoginBtn.click();
	}

	public void StopSession() throws InterruptedException {

		Thread.sleep(3000);
		WebElement Manage = driver.findElement(By.xpath("//button[contains(text(),'Manage')]"));
		JavascriptExecutor ja = (JavascriptExecutor) driver;
		ja.executeScript("arguments[0].click();", Manage);
		// driver.findElement(By.xpath("//button[contains(text(),'Manage')]")).click();
		Thread.sleep(1500);
		WebElement Stop = driver.findElement(By.xpath("//button[contains(text(),'Stop')]"));
		JavascriptExecutor jb = (JavascriptExecutor) driver;
		jb.executeScript("arguments[0].click();", Stop);
		// driver.findElement(By.xpath("//button[contains(text(),'Stop')]")).click();
		Thread.sleep(3000);

		driver.findElement(By.xpath("/descendant::button[@ng-click='logout($event,{withRedirect:true})'][2]")).click();
		// nedpPageTest.SessionLogout();
		Thread.sleep(3000);
		WebElement Exit = driver.findElement(By.xpath("/descendant::button[@ng-click='exit()'][2]"));
		JavascriptExecutor jc = (JavascriptExecutor) driver;
		jc.executeScript("arguments[0].click();", Exit);

	}

	
	
	
